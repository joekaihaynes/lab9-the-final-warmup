{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js$"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/minami",
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md",
    "encoding": "utf8"
  },
  "tags": {
    "allowUnknownTags": true
  }
}